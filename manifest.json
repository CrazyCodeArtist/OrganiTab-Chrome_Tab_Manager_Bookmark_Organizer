{
  "manifest_version": 3,
  "name": "OrganiTab - Chrome Tab Manager & Organizer",
  "version": "1.1",
  "description": "Save and manage tabs, groups, folders, bookmarks, and todos with a sleek interface and export/import functionality.",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus",
    "downloads"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
